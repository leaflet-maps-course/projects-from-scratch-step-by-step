{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "rootDir": "./",
    "paths": {
      "assets/*": [
        "./assets/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
  ]
}
